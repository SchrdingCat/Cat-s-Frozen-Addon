	Spawn:
		1CEA G 1 NODELAY ACS_NamedExecuteAlways("WadChecker",0,0,0,0)
		1CEA G 1
		TNT1 A 0 ACS_NamedExecuteAlways("SlaughtermapCheck")
		1CEA G 2
		1CEA G 0 A_RadiusGive("SlaughterCounter", 2000, RGF_MONSTERS, 4)
	Stand:
		1CEA G 25 A_Look
		Loop

