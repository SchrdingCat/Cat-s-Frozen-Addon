		TNT1 A 0 A_SetAngle(angle + 40)
		1CEA AA 10 A_Look
		1CEA A 0 A_PlaySound("baby/walk", 1)
		1CEA A 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA B 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA C 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA A 0 A_PlaySound("baby/walk", 1)
		1CEA D 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA E 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA F 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA DDEEFF 1 A_Wander
		TNT1 A 0 A_Jump(128, "SearchPlayer")
		1CEA A 0 A_PlaySound("baby/walk", 1)
		1CEA A 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA B 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA C 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA A 0 A_PlaySound("baby/walk", 1)
		1CEA D 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA E 3 A_Look
		TNT1 AA 0 A_Wander
		1CEA F 3 A_Look
