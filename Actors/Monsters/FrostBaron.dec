		TNT1 A 0 A_JumpIfInventory("SlaughterCounter", 200, "Standing")
		TROS A 0 A_RadiusGive("SlaughterCounter", 1000, RGF_MONSTERS, 1)
		Goto Stand
		
	Stand:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx("FrostBaron_NobleHeadshot",0,0,62,0,0,0,0,SXF_SETMASTER|SXF_NOCHECKPOSITION)
		TNT1 A 0 A_SpawnItemEx("BaronSmokeEffect",0,0,62,0,0,0,0,SXF_SETMASTER|SXF_NOCHECKPOSITION)
	Standing:
		FRBR D 20 A_Look
		Loop

	BDNOBOSSES:
		FRBR D 4
		TNT1 A 0 ACS_NamedExecuteAlways("BDCHeckClassicMonsters", 0, 0, 0, 0)//Check if Vanilla Mode is activated
		TNT1 A 0 A_JumpIfInventory("SlaughterCounter", 200, "Standing")
		TROS A 0 A_RadiusGive("SlaughterCounter", 1000, RGF_MONSTERS, 1)
		Goto Stand

	Death.Vanish:
		TNT1 A 0
		TNT1 A 0 A_Fall
		TNT1 A 1 A_BossDeath
		TNT1 A 0 A_KillChildren
		TNT1 A 0 A_NoBlocking
		Stop

	Check:
		TNT1 A 175//35 A_JumpIfHealthLower(0, "Execute")
		Loop

	Execute:
		TNT1 A 2 A_Die("Vanish")
		Goto Death.Vanish

	See:
		//TNT1 A 0 A_CheckSight("SeeNeverSeen")
		TNT1 AAA 0
		//TNT1 A 0 A_JumpIfInventory("HasExplosiveBarrel", 1, "MissileBarrel")
		FRBR D 1
		8TS3 ABC 1
		TNT1 A 0 A_FaceTarget
