	CheckRetreat:
		TNT1 A 0
		TNT1 A 0 A_JumpIfCloser(600, "Retreat")
		Goto Missile+5

	Retreat:
		TNT1 A 0
		TNT1 A 0 A_FaceTarget
		FRBR DD 2 A_Recoil(2)
		FRBR DD 2 A_Recoil(2)
		TNT1 A 0 A_FaceTarget
		FRBR CC 2 A_Recoil(1)
		FRBR CC 2 A_Recoil(1)
		FRBR BB 2 A_Recoil(1)
		FRBR BB 2 A_Recoil(1)
		FRBR AA 2 A_Recoil(1)
		FRBR AA 2 A_Recoil(1)
