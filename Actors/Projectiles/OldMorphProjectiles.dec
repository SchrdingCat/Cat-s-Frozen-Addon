//Rat FX=======================================================================
Actor RatShrinkSpawner
{
+NOINTERACTION	+NOBLOCKMAP	+NOGRAVITY
States
	{
	Spawn:
		TNT1 A 0
		TNT1 AAAAA 1 A_SpawnItemEx("RatShrinkFX")
        Stop
	}
}

Actor RatShrinkFX : RatShrinkSpawner
{
Scale 5
Alpha .75
States
	{
	Spawn:
		TNT1 A 0
		RATS A 1 A_Fadeout
		TNT1 A 0 A_SetScale(scaleX-.75)
        Loop
	}
}

Actor RatGrowSpawner : RatShrinkSpawner
{
States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_PlaySound("weapons/polyUnshoot")
		TNT1 AAAAA 1 A_SpawnItemEx("RatGrowFX")
        Stop
	}
}

Actor RatGrowFX : RatShrinkSpawner
{
Alpha .75
States
	{
	Spawn:
		TNT1 A 0
		RATS A 1 A_Fadeout
		TNT1 A 0 A_SetScale(scaleX+.5)
        Loop
	}
}
