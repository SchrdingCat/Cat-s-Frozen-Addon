ACTOR PurpleMorphFX
{
  +CLIENTSIDEONLY
  +NOINTERACTION
  +NOTIMEFREEZE
  var int user_loop;
  States
  {
 Spawn:
	TNT1 A 0 A_JumpIf(user_loop >= 3,"Null")	 
	TNT1 A 0 A_SetUserVar("user_loop",user_loop+1)
	TNT1 AAAAAAAAA 0 A_SpawnItemEx("PurpleMorphParticle",random(0,50),0,random(-20,20),frandom(10,30),0,frandom(-10,10),random(0,360))
	TNT1 AAAAAAA 0 A_SpawnItemEx("PurpleMorphExplosionSmoke",0,0,0,frandom(-20,20),frandom(-20,20),frandom(-10,10))
	TNT1 AAAAAA 0 A_SpawnItemEx("PurpleMorphSmoke2",0,0,0,frandom(-20,20),frandom(-20,20),frandom(-10,10))
	Loop
	}
}

