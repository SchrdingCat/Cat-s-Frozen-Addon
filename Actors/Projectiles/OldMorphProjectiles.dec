ACTOR PurpleMorphTrail
{
  +NOINTERACTION
  +CLIENTSIDEONLY
  +NOTIMEFREEZE
  Renderstyle Add
  States
  {
  Spawn:
    UNBL AAABBB 1 Bright A_FadeOut(0.1)
    Loop
	}
}

Actor PurpleMorphSmoke
{
  Scale 0.8
  +NOINTERACTION
  +CLIENTSIDEONLY
  +NOTIMEFREEZE
  +BRIGHT
  Renderstyle Add
  alpha 0.6
  States
  {
  Spawn:
	TNT1 A 0 NoDelay A_Jump(256,1,2,3)
	UNFG ABC 0 A_Jump(256,"Spawn2")
  Spawn2:
	"----" A 1 Bright A_FadeOut(0.06)
	Wait	 
	}
}

Actor PurpleMorphSmoke2 : PurpleMorphSmoke { Scale 1.0 }

Actor PurpleMorphExplosionSmoke : PurpleMorphSmoke
{
  Scale 1.2
  States
  {
  Spawn:
	UNB3 ABCDEF 3 Bright A_FadeOut(0.1)
	TNT1 A 0 A_PlaySOund("Decrep", 0, 1.0,0,ATTN_NONE)
	Stop	 
	}
}
 
