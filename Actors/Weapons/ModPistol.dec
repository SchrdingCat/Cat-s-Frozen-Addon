//laser pointer dots
ACTOR Laserdot
{ 
    Mass 0
    Scale 0.5
	alpha 0.7
	+nodecal
    +NOBLOCKMAP
    +NOGRAVITY
    +BLOODLESSIMPACT
    +ALWAYSPUFF
    +PUFFONACTORS
    +DONTSPLASH
    +FORCEXYBILLBOARD
	+NOTRIGGER
    States
    {
	Spawn:
		LSIT A 0
		LSIT A 0 A_CheckRange(1000,"Spawn3")
		LSIT A 0 A_CheckRange(550,"Spawn2")
		LSIT A 1 BRIGHT
		Stop
	Spawn2:
		LSIT A 0 A_SetScale(0.8,0.8)
		LSIT A 1 BRIGHT
		Stop
	Spawn3:
		LSIT A 0 A_SetScale(1.3,1.3)
		LSIT A 1 BRIGHT
		Stop
	}
}
ACTOR Laserdot2 : Laserdot
{ 
    States
    {
	Spawn:
		LSIT A 0
		LSIT A 0 A_CheckRange(1000,"Spawn3")
		LSIT A 0 A_CheckRange(550,"Spawn2")
		LSIT B 1 BRIGHT
		Stop
	Spawn2:
		LSIT A 0 A_SetScale(0.8,0.8)
		LSIT B 1 BRIGHT
		Stop
	Spawn3:
		LSIT A 0 A_SetScale(1.3,1.3)
		LSIT B 1 BRIGHT
		Stop
	}
}
ACTOR Laserdot3 : Laserdot
{ 
    States
    {
	Spawn:
		LSIT A 0
		LSIT A 0 A_CheckRange(1000,"Spawn3")
		LSIT A 0 A_CheckRange(550,"Spawn2")
		LSIT C 1 BRIGHT
		Stop
	Spawn2:
		LSIT A 0 A_SetScale(0.8,0.8)
		LSIT C 1 BRIGHT
		Stop
	Spawn3:
		LSIT A 0 A_SetScale(1.3,1.3)
		LSIT C 1 BRIGHT
		Stop
	}
}

